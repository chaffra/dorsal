NAME=SuiteSparse
SOURCE=http://www.cise.ufl.edu/research/sparse/SuiteSparse/current/
PACKING=.tar.gz
BUILDCHAIN=custom

package_specific_build () {
    export UMFPACK_DIR=$PWD
    wget http://glaros.dtc.umn.edu/gkhome/fetch/sw/metis/metis-4.0.tar.gz
    tar -xzf metis-4.0.tar.gz
    cd metis-4.0
    make
    cd ..
    make
}
