NAME=armadillo-2.2.2
SOURCE=http://downloads.sourceforge.net/arma/
PACKING=.tar.gz
BUILDCHAIN=custom

package_specific_build () {
    cmake -DCMAKE_INSTALL_PREFIX=${INSTALL_PATH} -DINSTALL_LIB_DIR=lib .
    make  -j ${PROCS}
    make install
}

package_specific_register () {
    export ARMADILLO_DIR=${INSTALL_PATH}
}
